#!/bin/sh
# prerm script for bunsen-configs

set -e

case $1 in
remove|deconfigure)
    update-alternatives --quiet --remove-all bl-file-manager
    update-alternatives --quiet --remove-all bl-image-viewer
    update-alternatives --quiet --remove-all bl-media-player
    update-alternatives --quiet --remove-all bl-text-editor
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
