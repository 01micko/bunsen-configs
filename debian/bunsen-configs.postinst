#!/bin/sh
# postinst script for bunsen-configs

set -e

case $1 in
configure)
    if [ -x /usr/bin/bl-alternatives ]
    then
        /usr/bin/bl-alternatives update
    fi
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0

